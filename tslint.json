{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-var-requires": false,
    "no-unused-expression": false,
    "no-console-error": false,
    "no-console": false,
    "no-empty": false,
    "max-line-length": [true, {
      "limit": 120,
      "ignore-pattern": "^import |^export {(.*?)}|class [a-zA-Z] implements|'|`"
    }],
    "align": false
  },
  "rulesDirectory": []
}
